<%= form_tag login_path do %>
  <p><%= label_tag :username%>
  <%= text_field_tag :username%>
  </p>
  <p>
  <%= label_tag :password%>
  <%= password_field_tag :password%>
  </p>
  <%= submit_tag "Login" %>
<% end %>
<br>
  <p>Don't have an account?</p><button><%= link_to "Sign Up!", '/signup' %></button>
